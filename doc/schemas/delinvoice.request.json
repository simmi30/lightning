{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "label",
    "status"
  ],
  "properties": {
    "label": {
      "oneOf": [
        {
          "type": "string",
          "description": ""
        },
        {
          "type": "integer",
          "description": ""
        }
      ]
    },
    "status": {
      "type": "string",
      "enum": [
        "paid",
        "expired",
        "unpaid"
      ]
    },
    "desconly": {
      "type": "boolean"
    }
  }
}
