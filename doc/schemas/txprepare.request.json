{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "outputs"
  ],
  "properties": {
    "outputs": {
      "type": "array",
      "items": {
        "type": "outputdesc"
      }
    },
    "feerate": {
      "type": "feerate"
    },
    "minconf": {
      "type": "u32"
    },
    "utxos": {
      "type": "array",
      "items": {
        "type": "outpoint"
      }
    }
  }
}
